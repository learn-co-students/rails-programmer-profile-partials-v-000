<%= link_to ''.html_safe + '<span class="glyphicon glyphicon-arrow-left" aria-hidden="true"></span>', root_path %>

<div class="row jumbotron">

  <div class="col-md-9 programmer-info">
    <%= render partial: 'attribute', locals: {attribute: 'home_country', value: @programmer.home_country} %>
    <%= render partial: 'attribute', locals: {attribute: 'quote', value: @programmer.quote} %>
    <%= render partial: 'attribute', locals: {attribute: 'claim_to_fame', value: @programmer.claim_to_fame} %>

    <p>
      <%= link_to "Learn more about #{@programmer.name}", @programmer.wikipedia_page, target: '_blank' %>
    </p>

  </div>
</div>
